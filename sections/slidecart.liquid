	.my-floating-cart .discount_flex .invest-txt {
			flex: 0 0 auto;
			width: 40%;
	}

	.my-floating-cart .discount_flex .percent_off {
			flex: 0 0 auto;
			width: 60%;
	}

	.my-floating-cart .discount_flex .progress_bar {
			flex: 0 0 auto;
			width: 100%;
	}

	.my-floating-cart h3.upsell_title {
			font-size: 16px;
			line-height: 26px;
			font-weight: 300;
	}

	.my-floating-cart .drawer__cart-subtotal p {
