<section class="drawer my-floating-cart" id="drawer-name">

	<a href="{{ routes.cart_url }}" data-ajax-cart-toggle-class-button="js-my-cart-open">
		<div class="drawer__overlay" tabindex="-1"></div>
	</a>
	<div class="drawer__wrapper" data-ajax-cart-section>

		<style>
			/* specical for desktop */
			@media only screen and (min-width:769px) {
				.my-floating-cart .drawer__cart-discounts {
					position: sticky !important;
					top: 46px !important;
					z-index: 1 !important;
				}
			}

			/* special for mobile */
			@media (max-width: 768px) {
				.my-floating-cart .drawer__cart-discounts {
					position: sticky !important;
					top: 36px !important;
					z-index: 1 !important;
				}
			}

			.custom-shadow {
				position: sticky !important;
				bottom: 0px !important;
				z-index: 1 !important;
			}
		</style>

		<form action="{{ routes.cart_url }}" method="post" class="my-cart cart_empty_class">

			{% for item in cart.items %}
