									{% else %}
										{% comment %} added by abconvert 31.08.2023 {% endcomment %}
										<a  class="drawer__cart-title"  href="{{ item.url }}">{{ item.title | replace: "- Default Title","" | replace: "*","" }}{% if free_gift %} (Free gift){% endif %}</a>
									{% endif %}
								{% else %}
