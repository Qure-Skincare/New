	.my-floating-cart .drawer__cart-total-price .old_price-cart {
			font-weight: 700;
			color: #C4C4C4;
	}

	.my-floating-cart .drawer__cart-total-price .old_price-cart,
	.my-floating-cart .drawer__cart-total-price .cart_items_subtotal_price {
			letter-spacing: 0.24px;
	}

	.my-floating-cart .payment_info {
			display: flex;
			align-items: center;
			justify-content: space-around;
			max-width: 375px;
			margin: 9px auto;
