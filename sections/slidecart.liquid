{%- assign current_variant = product.selected_or_first_available_variant -%}
{%- assign featured_image = product.selected_or_first_available_variant.featured_image | default: product.featured_image-%}

{% assign progress_Q = 0 %}
{% assign progress_A =0 %}
{% assign progress_M = 0 %}
{% assign is_only_gift = false %}
{% assign price_check=cart.items_subtotal_price | divided_by: 100 %}

{% assign shipping_charge =  1000.00 %}

{% render "slidecart-insurance" %}

<style>
	.my-floating-cart .cart_total-checkout {
			position: relative !important
	}

	.my-floating-cart .percent_off {
			padding: 0 !important;
	}

	.my-floating-cart .percent_off svg {
			margin-right: 5px;
	}

	.my-floating-cart p.invest-txt {
			padding: 0;
	}

	.my-floating-cart .drawer__cart-discounts {
			padding: 20px 30px;
	}

	.my-floating-cart .progress_bar {
			padding: 20px 0 0px 0;
	}

	.my-floating-cart .discount__line {
			opacity: 1;
			border-top: 1px solid #ffffff;
	}

	.my-floating-cart .skill-bar .progress {
			/* background-color: #C7E7DC; */
	}

	.my-floating-cart .drawer_top_sticky {
			position: sticky;
			top: 0;
			z-index: 9;
