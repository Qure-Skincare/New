									{% if cart.item_count == 0  %}
									You’re <i class="money" style="font-style: normal;">$100</i> away from free shipping
									{% elsif price_check < 100 and price_check > 0 %}
									Spend <i class="money" style="font-style: normal;">${{ price_check | minus: 100 | abs }}</i> more to get Free Shipping.
									{% else %}
									Congrats! Shipping is on us.
									{% endif %}
								</span>
							</p>
						</div>
					</div>
				</div>
			</div>

			{% render 'slidecart-timer' %}

			<div class="my-cart__info">
				<div  class ="my-cart__items {% if cart.item_count == 0 %} cart__empty-items {% endif %}">
					{%- assign is_not_only_gift = false -%}
					{% for item in cart.items %}
						{% assign item_index = forloop.index %}
						{% assign item_new_price = item.quantity | times: item.variant.price  %}
						{% assign item_old_price = item.quantity | times: item.variant.compare_at_price  %}

						{% assign free_gift = false %}

						{% if item.properties._free_product == 'true' %}
							{% assign free_gift = true %}
						{%- endif -%}


						{%- if item.title contains 'Gift Card' -%}
							{%- unless is_not_only_gift -%}
								{%- assign is_only_gift = true -%}
							{%- endunless -%}
						{%- else -%}
							{%- assign is_only_gift = false -%}
							{%- assign is_not_only_gift = true -%}
						{%- endif -%}

						{%- if item.variant.compare_at_price -%}
							{% assign compare_price = item.variant.compare_at_price | times: item.quantity %} 
						{%- else -%}
							{%- assign compare_price = item.variant.price -%}
						{%- endif -%}

						{% assign total_cart_compare_price = total_cart_compare_price | plus: compare_price %}

						<div class="drawer__cart-product">
							<div class="drawer__cart-product-image">
								{%- if item.title contains 'Insurance' -%}
									<a title="{{ item.title }}" href="{{ item.url }}"><img src="{{ item.image | img_url: 'master' }}" style="height:20px !important;"></a>
								{%- elsif item.title contains 'Gift Card' -%}
									<a title="{{ item.title }}" href="{{ item.url }}"><img src="{{ item.image | img_url: 'master' }}" style="height:60px !important; margin-top:10px; !important"></a>
								{%- else -%}
									<a title="{{ item.title }}" href="{{ item.url }}"><img src="{{ item.image | img_url: 'master' }}"></a>
								{%- endif -%}
							</div>
