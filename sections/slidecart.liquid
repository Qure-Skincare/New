					{% assign progress_Q = 1 %}
				{% endif %}

				{% if  item.title contains 'All-In-One Face Serum'  %}
					{% assign  progress_A = 1  %}
				{% endif %}
					
				{% if item.title contains "Micro-Infusion" %}
					{% assign  progress_M = 1  %}
				{% endif %}
			{% endfor %}
				
			{% if price_check > 100 %}
				{% assign progress_percentage = 100 %}
			{% else %}
				{% assign progress_percentage = price_check %}
			{% endif %}

			<div class="drawer_top_sticky">
				<div class="drawer__count">
					<p class="your-cart" >Your Cart (<span class="your-cart" data-ajax-cart-bind-state="cart.item_count">{{ cart.item_count  }}</span>)</p>
					<a href="{{ routes.cart_url }}" data-ajax-cart-toggle-class-button="js-my-cart-open">
						<button class="drawer__close drawer__close-header">
							<svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg">
								<path d="M2 15.9998L16 1.99976" stroke="white" stroke-width="3"></path>
								<path d="M14.7273 15.2393L8.36364 8.87562L2 2.51199" stroke="white" stroke-width="3"></path>
							</svg>
						</button>
					</a>
				</div>
				<div class="drawer__cart-discounts_wrap">
					<hr class="discount__line">
					<div class="drawer__cart-discounts">
						<div class="discount_flex">
							<p class="invest-txt order-1">Your skincare investment is waiting.</p>
							<style>
								.percent_off {
									text-align: right;
									padding: 20px 30px !important;
								}

								.percent_off span {
									border-radius: 100px;
									background: #333;
									padding: 6px 10px 6px 10px;
								}

								.percent_off img {
									width: 16px;
									height: 16px;
									margin: 0px 3px 0px 3px;
								}
							</style>
							<div class="progress_bar order-2 order-md-3">
								<div class=" skill-bar">
									<div class="line">
										<div class="each-line">
											<div class="drawer__cart-free-shipping" style="width:0px;"></div>
											<div class="progress" style="width: 100% !important; border-radius:100px;background-color:#c4c4C480;">
												<div class="determinate" style="width:{{ progress_percentage }}%; border-radius:100px !important;"></div>
											</div>
											<div class="icon_bar" style="width:0px;">
												<div class="percentage"></div>
											</div>
										</div>
									</div>
								</div>
							</div>
							<p class="percent_off order-3 order-md-2">
								<span>
									<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16" fill="none">
